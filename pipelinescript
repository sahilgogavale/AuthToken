pipeline {
  agent any

  stages{

    stage('Build'){
      steps{
        echo "file created ${params.filename}"
    }
  }
  stage('Run'){
    steps{
      bat "echo 'i am sahil' > ${params.filename}"
    }
  }
}
}

http://localhost:8080/buildByToken/buildWithParameters?token=1234&job=h&whoami=pranit&filename=test.txt
